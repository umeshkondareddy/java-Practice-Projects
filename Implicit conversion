
public class typeconversion {

	public static void main(String[] args)  
	{ 
// implicit conversion
	int a = 7;  
	long b = a;  
	double c = b;  
	System.out.println("Before conversion, int value "+a);  
	System.out.println("After conversion, long value "+b);  
	System.out.println("After conversion, double value "+c);  
	}  
	}  

public class explicitconversion {
	public static void main(String args[])  
	{  
	double a = 567.999;   
	long b = (long)a;  
	int c = (int)b;  
	System.out.println("Before conversion: "+a);   
	System.out.println("After conversion into long type: "+b);    
	System.out.println("After conversion into int type: "+c);  
	}  
}
