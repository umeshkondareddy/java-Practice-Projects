//Private access modifier
class Data 
{
    String name;
}

public class privatemodifier 
{
    public static void main(String[] main)
    {
        Data d = new Data();
        d.name = "umesh";
    }
}

//public access modifier
public class umesh
{
	public int count;
    public void display() 
    {
        System.out.println("my self umesh.");
        System.out.println("Im doing " + count + " projects.");
    }
}

public class main 
{
    public static void main( String[] args ) 
    {
        umesh u = new umesh();

        u.count =2;
        u.display();
    }
}

